{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "20.x",
        "installCommand": "rm -f pnpm-lock.yaml && npm install --package-lock-only && npm install",
        "runtime": "nodejs20.x",
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ],
  "regions": ["fra1"],
  "env": {
    "ENABLE_VC_BUILD": "true"
  }
}