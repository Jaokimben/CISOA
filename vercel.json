{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "curl -f https://get.pnpm.io/v6.16.js | node - add --global pnpm && pnpm install",
        "nodeVersion": "20",
        "includeFiles": [".vercel/pnpmrc"],
        "runtime": "nodejs20.x",
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ],
  "regions": ["fra1"],
  "env": {
    "ENABLE_VC_BUILD": "true"
  }
}