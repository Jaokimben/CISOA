{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "20.x",
        "installCommand": "rm -rf node_modules pnpm-lock.yaml && npm install --package-lock-only --force && npm install",
        "forceNodeVersion": "20.x",
        "npmVersion": "10.x",
        "runtime": "nodejs20.x",
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/_app/immutable/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "frontend",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "regions": ["fra1"],
  "env": {
    "ENABLE_VC_BUILD": "true"
  },
  "outputDirectory": "frontend/build"
}