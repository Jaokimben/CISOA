{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "corepack enable && pnpm install --frozen-lockfile",
        "nodeVersion": "20.x",
        "installCommand": "rm -rf node_modules pnpm-lock.yaml && pnpm install --frozen-lockfile",
        "forceNodeVersion": "20.x",
        "pnpmVersion": "8.15.7",
        "runtime": "nodejs20.x",
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/_app/immutable/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "regions": ["fra1"],
  "env": {
    "ENABLE_VC_BUILD": "true"
  },
  "outputDirectory": "frontend/.svelte-kit/output/client",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}